    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --transformIgnorePatterns \"/node_modules/(?!(@polkadot|@babel|react-calendar|react-date-picker)/).*\"",
        "report-test-coverage": "react-scripts test --transformIgnorePatterns \"/node_modules/(?!(@polkadot|@babel|react-calendar|react-date-picker)/).*\" --coverage --watchAll",
        "eject": "react-scripts eject",
        "generate:defs-meta": "yarn generate:defs && yarn generate:meta",
        "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --input ./src/logion-chain/interfaces/ --package ../interfaces",
        "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --endpoint ws://localhost:9944 --output src/logion-chain/interfaces/ --package ../interfaces",
        "pre-release": "scripty",
        "analyze": "source-map-explorer 'build/static/js/*.js'"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not ie <= 99",
            "not android <= 4.4.4",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.16.12",
