        "@testing-library/user-event": "^14.1.1",
        "@types/jest": "^27.0.3",
        "@types/node": "^17.0.14",
        "@types/react": "^18.0.5",
        "@types/react-dom": "^18.0.1",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-test-renderer": "^18.0.0",
        "@types/sha.js": "^2.4.0",
        "@types/sjcl": "^1.0.30",
        "@types/uuid": "^8.3.3",
        "autoprefixer": "^10.4.0",
        "http-proxy-middleware": "^2.0.2",
        "jest": "^27.4.5",
        "mini-css-extract-plugin": "2.6.0",
        "moq.ts": "^7.3.4",
        "node-sass": "^7.0.0",
